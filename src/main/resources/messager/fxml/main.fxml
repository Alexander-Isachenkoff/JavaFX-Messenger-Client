<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox prefHeight="720" prefWidth="1200" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.171"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="messager.controller.MainController">
    <children>
        <HBox alignment="TOP_RIGHT">
            <children>
                <Button mnemonicParsing="false" onAction="#onAdd" text="+"/>
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </padding>
        </HBox>
        <TabPane fx:id="tabPane" tabMaxWidth="120.0" tabMinWidth="120.0" VBox.vgrow="ALWAYS"/>
    </children>
</VBox>
