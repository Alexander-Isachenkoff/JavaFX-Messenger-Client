<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Line?>
<VBox alignment="TOP_RIGHT" prefHeight="360.0" prefWidth="480.0" spacing="10.0"
      style="-fx-background-radius: 10; -fx-border-radius: 10; -fx-border-color: lightgray; -fx-border-width: 1;"
      stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="messager.controller.AddDialogController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <children>
        <Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                mnemonicParsing="false" onAction="#onClose" prefHeight="19.0" prefWidth="19.0" styleClass="tool-button">
            <graphic>
                <Group>
                    <children>
                        <Line endX="10.0" endY="10.0" styleClass="line"/>
                        <Line endX="10.0" startY="10.0" styleClass="line"/>
                    </children>
                </Group>
            </graphic>
        </Button>
        <TextField fx:id="searchField" promptText="Поиск"/>
        <ListView fx:id="usersListView" snapToPixel="false" styleClass="users-list" VBox.vgrow="ALWAYS"/>
    </children>
</VBox>
